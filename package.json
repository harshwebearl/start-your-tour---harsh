{
  "name": "start-your-tour",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "start": "node ./bin/www",
    "dev": "SET NODE_ENV=development & nodemon ./bin/www",
    "prod": "SET NODE_ENV=production & nodemon ./bin/www",
    "prepare": "husky install",
    "prettier:check": "npx prettier --check .",
    "prettier:write": "npx prettier --write .",
    "start-gendoc": "SET NODE_ENV=development & node swagger.js"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "_lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.3",
    "aws-sdk": "^2.1290.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.6.1",
    "easyinvoice": "^2.3.3",
    "ejs": "~2.6.1",
    "env": "^0.0.2",
    "exceljs": "^4.3.0",
    "express": "~4.16.1",
    "express-oas-generator": "^1.0.46",
    "firebase-admin": "^11.0.1",
    "http-errors": "~1.6.3",
    "json-bigint": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.1",
    "mongoose-aggregate-paginate-v2": "^1.0.6",
    "mongoose-paginate-v2": "^1.7.1",
    "mongoose-to-swagger": "^1.4.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "node-input-validator": "^4.5.0",
    "nodemailer": "^6.7.2",
    "nodemon": "^2.0.22",
    "pdfkit": "^0.13.0",
    "quill-delta-to-html": "^0.12.1",
    "sha256": "^0.2.0",
    "socket.io": "^4.7.5",
    "swagger-autogen": "^2.22.0",
    "twilio": "^3.84.0",
    "uniqid": "^5.4.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "engines": {
    "npm": ">=6.x",
    "node": ">=16.13.0"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.0.3",
    "patch-package": "^6.5.1",
    "prettier": "2.7.1",
    "swagger-jsdoc": "^6.2.0",
    "swagger-ui-express": "^4.6.0"
  }
}
